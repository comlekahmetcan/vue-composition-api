rks?_expand=category&_expand=user&categoryId=${categoryId}` : `/bookmarks?_expand=category&_expand=user`;\n//   appAxios.get(url).then((bookmark_list_response) => {\n//     bookmarkList.value = bookmark_list_response?.data || [];\n//   });\n// };\n\nfetchData();\n</script>\n"]}]}