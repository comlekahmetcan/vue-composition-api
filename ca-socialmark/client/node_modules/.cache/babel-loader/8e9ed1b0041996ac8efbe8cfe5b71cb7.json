 = ref({\n  username: null,\n  fullname: null,\n  password: null,\n});\n\nconst onSave = () => {\n  const password = CryptoJS.HmacSHA1(userData.value.password, store.getters._saltKey).toString();\n  appAxios.post(\"/users\", { ...userData.value, password }).then(() => {\n    router.push({ name: \"HomePage\" });\n  });\n};\n</script>\n"],"sourceRoot":""}]}